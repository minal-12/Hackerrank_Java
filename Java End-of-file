import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        String s;
        int n=1;
       Scanner in=new Scanner(System.in);
       while(in.hasNext())
       {
            System.out.println(n+" "+in.nextLine());
            n++;
       }
       in.close();
       
       
    }
}
